#%Module4.5

# check if OSRELEASE is set
if { [info exists env(OSRELEASE)] } {
  # print warning message
  puts stderr "Warning: OSRELEASE is already set to $env(OSRELEASE)."
} else {
  setenv OSRELEASE [ exec $env(SIM_HOME)/ceInstall/osrelease.py ]
}

#setenv OSRELEASE [ exec $env(SIM_HOME)/ceInstall/osrelease.py ]
setenv SIM_INSTALLATION_DIR $env(SIM_HOME)/$env(SIM_VERSION)/$env(OSRELEASE)


