#%Module

set version [file tail [module-info version [module-info name]]]

prereq .clhep/2.4.7.1
prereq .xercesc/3.2.5
#prereq .qt/system # not needed as long as Qt5_DIR is set to where the Qt5Config.cmake is

# datasets. must be env variable for install_geant4 data to work properly
setenv g4ndl_version               4.7.1
setenv g4emlow_version             8.6.1
setenv g4photonevaporation_version 6.1
setenv g4radioactivedecay_version  6.1.2
setenv g4particlexs_version        4.1
setenv g4pii_version               1.3
setenv g4real_surface_version      2.2
setenv g4saidata_version           2.0
setenv g4abla_version              3.3
setenv g4incl_version              1.2
setenv g4ensdfstate_version        3.0
setenv g4tendl_version             1.4

source [file dirname $ModulesCurrentModulefile]/.common
