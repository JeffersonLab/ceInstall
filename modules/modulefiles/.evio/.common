#%Module

conflict .evio

proc ModulesHelp { } {
    global version
    puts stderr "
    Loads evio environment variables
    Version: $version
"
}

module-whatis "Loads evio/${version}"

# Environment

set base_dir $env(PHYS_HOME)/$env(OSRELEASE)/$env(PHYS_SUB_DIR)/$env(PHYS_VERSION)

set dir $base_dir/evio/${version}

prepend-path LD_LIBRARY_PATH $dir/lib

setenv EVIO_VERSION  $version
setenv EVIO $dir
